<div class="container">
  <div class="row justify-content-center my-5">
    <div class="col-6">

      <h1>New List</h1>
        <%= simple_form_for [@list || List.new] do |f| %>

          <%= f.input :name %>
          <%= f.input :photo, as: :file %>

          <%= f.submit "Submit", class: "btn btn-primary" %>
        <% end %>
    </div>
  </div>
</div>
