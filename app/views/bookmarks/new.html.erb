<div class="container">
  <div class="row justify-content-center my-5">
    <div class="col-6" style ="color:white">

      <h1>New Bookmark</h1>
        <%= simple_form_for [@list, @bookmark] do |f| %>
          <%= f.association :movie %>
          <%= f.input :comment %>
          <%= f.submit "Submit", class: "btn btn-primary" %>
        <% end %>
    </div>
  </div>
</div>
